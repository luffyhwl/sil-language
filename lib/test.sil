print("测试while for break; continue \n");
var i=0;
while(i<7)
{
	if(i==1)
	{
		print("if(i==1)"+i+"\n");
	}
	else if(i==2)
	{
		print("if(i==2)"+i+"\n");
	}
	else if(i==3) print("if(i==3)"+i+"\n");
	else if(i==4)
	{
		print("if(i==4)"+i+"\n");
	}
	else if(i==5)
	{
	break;
	}
	else if(i==6) break;
	else print("else "+i+"\n");
	i=i+1;
}

for(var i=0;i<5;i=i+1)
{
	print("for1 "+i+"\n");
}
i=0;
for(;i<50;i=i+1)
{
	print("for2 "+i+"\n");
